{
    "name": "community-sdks/godaddy-php",
    "description": "PHP SDK for the GoDaddy APIs in this repository.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "CommunitySDKs\\GoDaddy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CommunitySDKs\\GoDaddy\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml.dist"
    }
}
